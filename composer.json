{
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.1",
        "illuminate/cache": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/view": "^8.0",
        "woocommerce/action-scheduler": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "HandmadeWeb\\Illuminate\\": "src"
