{
    "require": {
        "php": "^7.0 | ^8.0",
        "doctrine/dbal": "^3.1",
        "illuminate/cache": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/view": "^8.0",
        "handmadeweb/hookable-actions-filters": "^1.0",
        "symfony/var-dumper": "^5.2",
        "woocommerce/action-scheduler": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "HandmadeWeb\\Illuminate\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
