{
    "require": {
        "php": "^7.2",
        "illuminate/cache": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/view": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "HandmadeWeb\\Illuminate\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
